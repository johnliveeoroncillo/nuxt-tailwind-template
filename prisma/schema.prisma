generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  Id       Int      @id @default(autoincrement()) // Primary key
  Name     String   @db.VarChar(50)
  Surname  String
  Email    String   @unique
  Password String
  CreateAt DateTime @default(now())
}
