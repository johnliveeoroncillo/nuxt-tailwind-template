{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "nuxt start",
    "cap:sign": "apksigner sign --ks ./android/cloudcarelabs.jks --ks-key-alias cloudcarelabs --out ./android/app/build/outputs/apk/release/app-release-signed-apksigner.apk ./android/app/build/outputs/apk/release/app-release-signed.apk",
    "cap:store": "keytool -genkey -v -keystore cloudcarelabs.jks -alias cloudcarelabs -keyalg RSA -keysize 2048 -validity 10000",
    "cap:sync:android": "npx cap sync android",
    "cap:sync:ios": "npx cap sync ios",
    "cap:build:android": "npx cap build android",
    "cap:build:ios": "npx cap build ios",
    "build:android": "npm run generate && npm run cap:sync:android && npm run cap:build:android && npm run cap:sign",
    "cap:add:android": "npx cap add android",
    "cap:add:ios": "npx cap add ios"
  },
  "dependencies": {
    "@capacitor/android": "^7.0.1",
    "@capacitor/core": "^7.0.1",
    "@capacitor/ios": "^7.0.1",
    "@nuxtjs/color-mode": "^3.5.2",
    "@pinia/nuxt": "^0.9.0",
    "@radix-icons/vue": "^1.0.0",
    "@vite-pwa/nuxt": "^0.10.6",
    "@vueuse/core": "^12.5.0",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "js-cookie": "^3.0.5",
    "lucide-vue-next": "^0.474.0",
    "nuxt": "^3.15.4",
    "pinia": "^2.3.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "pulltorefreshjs": "^0.1.22",
    "radix-vue": "^1.9.13",
    "shadcn-nuxt": "^0.11.3",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "vaul-vue": "^0.2.1",
    "vue": "latest",
    "vue-router": "latest",
    "vue-sonner": "^1.3.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.0.1",
    "@nuxtjs/tailwindcss": "^6.13.1",
    "@samk-dev/nuxt-vcalendar": "^1.0.4",
    "@types/crypto-js": "^4.2.2",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.13.5",
    "@types/pulltorefreshjs": "^0.1.7",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.3",
    "sass-embedded": "^1.83.4",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  }
}
